<div id="vantajs">

    <style>
        #vantajs {
        position:absolute;
        top: 55px; 
        bottom: 20px;
        width: 100%;
        z-index: -9912;
    }
    </style>
    
    <script>
    VANTA.BIRDS({
      el: "#vantajs",
      mouseControls: true,
      touchControls: true,
      gyroControls: false,
      minHeight: 200.00,
      minWidth: 200.00,
      scale: 1.00,
      scaleMobile: 1.00,
      backgroundColor: 0xede344,
      birdSize: 1.20,
      wingSpan: 33.00,
      speedLimit: 6.00,
      separation: 38.00
    });
    
    /* code highlight */
    $(document).ready(function(){
      hljs.initHighlightingOnLoad();
    });
        </script>
    
<style>
    .col{ 
        padding: 10px 20px;
    }
    .containerCover  { 
      margin: 0;
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
    }
    @media (max-width:1110px) {
        .containerCover  { 
      margin: 18.5px;
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
    }   
    }
</style>

